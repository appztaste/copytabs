{
    "name":"CopyTabs",
    "version":"1.0",
    "description":"copy current tab urls",
    "manifest_version":2,
    "permissions":["clipboardWrite","tabs"],
    "browser_action": {
        "default_icon": {                    
          "16": "images/icon16.png",          
          "32": "images/icon32.png"            
        },
        "default_title": "Copy Tabs",
        "default_popup":"popup.html"
      },
      "icons": {
        "16": "images/icon16.png",          
        "32": "images/icon32.png"
      }
}