{
    "name":"CopyTabs",
    "version":"1.0",
    "description":"copy current tab urls",
    "manifest_version":2,
    "permissions":["clipboardWrite","tabs"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action": {
        "default_icon": {                    
          "16": "images/icon16.png",          
          "32": "images/icon32.png"            
        },
        "default_title": "Copy Tabs"
      }
}